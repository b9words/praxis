{
  "name": "praxis-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --webpack -p 3400",
    "build": "next build --webpack",
    "build:analyze": "ANALYZE=true next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "import-content": "tsx scripts/import-content.ts",
    "content:sync": "tsx scripts/import-content.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed-competencies": "tsx scripts/seed-competencies.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=json --outputFile=coverage.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:analyze": "tsx scripts/analyze-test-results.ts",
    "test:full": "npm run test:e2e && npm run test:analyze",
    "test:fix-cycle": "tsx scripts/test-fix-cycle.ts",
    "audit:data-layer": "bash scripts/audit.sh",
    "db:reset": "prisma db push --force-reset",
    "db:seed:test": "psql $DATABASE_URL -f tests/seed-data.sql"
  },
